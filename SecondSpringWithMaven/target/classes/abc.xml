   </bean>
   
    <bean id = "empp" class = "com.incapp.Employee">
	  <constructor-arg type="String" value="Rahul Sir"/>
	  <constructor-arg type="String" value="Incapp"/>
      <constructor-arg type="int" value="50000"/>
   </bean>
   
   
</beans>